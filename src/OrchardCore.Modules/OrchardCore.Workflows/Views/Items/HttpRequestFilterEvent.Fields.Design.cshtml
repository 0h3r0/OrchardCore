@using OrchardCore.Workflows.Activities
@{
    var activity = (HttpRequestFilterEvent)Model.Activity;
}
<header>
    <h4><i class="fa fa-bolt"></i>@T["Filter Incoming HTTP Request"]</h4>
</header>
<span>@T["Filter <em>{0} with route values {1}</em>", activity.HttpMethod, activity.RouteValues.ToString()]</span>